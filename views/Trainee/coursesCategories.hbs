<!DOCTYPE html>
<html lang="en">

{{> head}}


<body>
    <div class="app">
        {{> traineeHeader}}
        <div class="container">
            <div class="grid">
                <div class="row">
                    <h1 class="text-uppercase text-center mt-3">Course Category</h1>
                   
                    <div class="e-panel card" style="background-color: #f5f5f5">
                        <div class="card-body">
                            <div class="card-title">
                            </div>
                            <div class="e-table">
                                <div class="table-responsive table-lg mt-3">
                                    <table class="table table-bordered">
                                        <thead style="text-align: center;">
                                            <tr>
                                                <th>TITLE</th>
                                            </tr>
                                        </thead>
                                        <tbody style="text-align: center;">
                                            {{#each listCategory}}
                                            <tr>
                                                <td class="text-nowrap align-middle">{{name}}</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <script type="text/javascript"
            src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.6.0/mdb.min.js"></script>
</body>

</html>